<!-- _TestReportsPartial.cshtml -->
@model IEnumerable<Hospital_Management.Areas.Patient.Models.TestReports>

@if (Model != null && Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Test Report ID</th>
                <th>User ID</th>
                <th>Doctor Name</th>
                <th>Test Category</th>
                <th>Test Date</th>
                <th>Test Result</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var report in Model)
            {
                <tr>
                    <td>@report.TestReportId</td>
                    <td>@report.UserId</td>
                    <td>@report.DoctorName</td>
                    <td>@report.TestCategory</td>
                    <td>@report.TestDate</td>
                    <td>@report.TestResult</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No test reports available.</p>
}
